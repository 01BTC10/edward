{
  "name": "edward",
  "version": "4.1.0",
  "description": "web editor based on Ace",
  "main": "server/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudcmd/edward"
  },
  "keywords": [
    "editor",
    "express",
    "middleware"
  ],
  "author": "coderaiser",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cloudcmd/edward/issues"
  },
  "homepage": "https://github.com/cloudcmd/edward",
  "scripts": {
    "build:server": "redrun 6to5:*",
    "watch:server": "nodemon -w server -x bin/dword.js package.json",
    "6to5:lib": "babel server -d server_",
    "6to5:bin": "babel bin -d legacy/bin",
    "build": "redrun build:* legacy:*",
    "legacy:index": "echo \"module.exports = require('../server_');\" > legacy/index.js",
    "wisdom": "npm run build"
  },
  "bin": {
    "edward": "bin/edward.js",
    "edward_": "legacy/bin/edward.js"
  },
  "dependencies": {
    "ashify": "^1.0.1",
    "currify": "^2.0.0",
    "express": "^4.14.0",
    "join-io": "^1.4.0",
    "mollify": "^1.0.4",
    "os-homedir": "^1.0.0",
    "patchfile": "^1.0.1",
    "readjson": "^1.1.0",
    "rendy": "^1.1.0",
    "restafary": "^1.7.0",
    "socket-file": "^1.3.0",
    "socket.io": "^1.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "bower": "^1.5.1",
    "bupdate": "^1.0.4"
  }
}
